{
  "$id": "jira_Issue",
  "$schema": "https://schema.yext.com/config/km/field-presentation/v1",
  "entityType": "jira_Issue",
  "displayName": "Jira Issue",
  "presentationEntries": [
    {
      "displayName": "Content",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Core Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "name",
          "visibleOnAdd": true
        },
        {
          "id": "entityId",
          "visibleOnAdd": true
        },
        {
          "id": "body"
        },
        {
          "id": "jira_reporterEmail"
        },
        {
          "id": "jira_reporterName"
        },
        {
          "id": "jira_engineeringTeam"
        },
        {
          "id": "jira_assigneeName"
        },
        {
          "id": "jira_assigneeEmail"
        },
        {
          "id": "jira_status"
        },
        {
          "id": "landingPageUrl"
        },
        {
          "id": "jira_comments"
        }
      ]
    },
    {
      "displayName": "Other",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Internal Use Only",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "folder"
        },
        {
          "id": "labels"
        }
      ]
    }
  ]
}
